{
  "name": "LatinApp",
  "version": "1.0.0",
  "description": "latin to english and english to latin translator",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/halihuang/LatinWiki.git"
  },
  "main": "index.js",
  "scripts": {
    "start": "npm run serve",
    "serve": "http-server ./www/ -o -c 1 -a localhost",
    "copy-f7": "cpy node_modules/framework7/js/*.* www/framework7/js && cpy node_modules/framework7/css/*.* www/framework7/css && cpy node_modules/framework7-icons/fonts/*.* www/fonts && cpy node_modules/material-design-icons/iconfont/*.{eot,ttf,woff,woff2} www/fonts",
    "postinstall": "npm run copy-f7"
  },
  "keywords": [
    "framework7",
    "template",
    "ios",
    "android",
    "mobile",
    "app",
    "cordova",
    "phonegap"
  ],
  "author": "Hali Huang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/framework7io/framework7-template-split-view/issues"
  },
  "homepage": "https://github.com/framework7io/framework7-template-split-view#readme",
  "devDependencies": {
    "cordova-plugin-whitelist": "^1.3.4",
    "cpy-cli": "^1.0.1",
    "framework7": "^4.5.2",
    "framework7-icons": "^2.3.1",
    "material-design-icons": "^3.0.1"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "cordova-android": "^8.0.0",
    "cordova-ios": "^5.0.1",
    "ecstatic": "4.1.2",
    "http-server": "0.9.0",
    "jsonfile": "^6.0.1"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {}
    },
    "platforms": [
      "android",
      "ios"
    ]
  }
}
